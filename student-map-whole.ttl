@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .

@base <http://example.com/base/> .

<TriplesMap1>
  a rr:TriplesMap;
        
  rml:logicalSource [ 
    rml:source "student.csv";
    rml:referenceFormulation ql:CSV
  ] ;
	
  rr:subjectMap [ 
    rr:template "http://example.com/{ID}/{Name}";
    rr:class foaf:Person 
  ];

  rr:predicateObjectMap [ 
    rr:predicate ex:id ; 
    rr:objectMap [ rml:reference "ID" ]
  ];

  rr:predicateObjectMap [ 
    rr:predicate foaf:name ; 
    rr:objectMap [ rml:reference "Name" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:title ; 
    rr:objectMap [ rml:reference "Title" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:department ; 
    rr:objectMap [ rml:reference "Department" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:department ; 
    rr:objectMap [ rml:reference "Phones" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:department ; 
    rr:objectMap [ rml:reference "Emails" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:department ; 
    rr:objectMap [ rml:reference "Websites" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:image ; 
    rr:objectMap [ rml:reference "Image" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:overview ; 
    rr:objectMap [ rml:reference "Overview" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:twitter ; 
    rr:objectMap [ rml:reference "Twitter" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:gatorlink ; 
    rr:objectMap [ rml:reference "Gatorlink" ]
  ];
  
    rr:predicateObjectMap [ 
    rr:predicate ex:grantPIOf ; 
    rr:objectMap [ rml:reference "GrantPI" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:grantCoIOf ; 
    rr:objectMap [ rml:reference "GrantCoI" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:grantMemberOf ; 
    rr:objectMap [ rml:reference "GrantMember" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:authorOf ; 
    rr:objectMap [ rml:reference "PMIDs" ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate ex:instructorOf ; 
    rr:objectMap [ rml:reference "Courses" ]
  ]
  
.
